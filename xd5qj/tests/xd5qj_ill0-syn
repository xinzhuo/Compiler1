//Create base on the legal test case
//No declaration of alpha
VAR beta;
VAR gamma;
VAR sigma;
VAR random;

alpha = 30 * 2 + 6.78;
GO cos alpha; 
//the ID alpha was not created in the decl section, thus an error would appear if the ID is called

random = alpha/2.3;

FOR beta = 2/2 STEP 2 
TO 20
DO
BEGIN
	JUMP random;
	GO sqrt random;
	random = 3*2;
	TURN 4;
END;

sigma = (alpha * 3.2) + (sin 3) - 5.0;
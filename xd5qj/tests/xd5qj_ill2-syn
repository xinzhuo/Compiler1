GO cos alpha;

random = alpha/2.3;

FOR beta = 2/2 STEP 2 
TO 20
DO
BEGIN
	JUMP random;
	random ++; //undefined statement
	GO sqrt random;
	random = 3*2;
	TURN 4;
END;

sigma = (alpha * 3.2) + (sin 3) - 5.0
